<?xml version="1.0" encoding="utf-8"?>
<connectionStrings>
  <!--SQL server configuration manager 查看是否开启对应协议-->
  <!--Shared Memory控制本机访问本机的实例-->
  <add name="sqlconn" connectionString="Data Source=.;Initial Catalog=RightControl; User Id=sa;Password=sqlserver2008"/>
